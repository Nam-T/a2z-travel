# -*- mode: ruby -*-
# vi: set ft=ruby :
create_table 'profiles', id: :integer, force: :cascade, options: 'ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_bin ROW_FORMAT=DYNAMIC' do |t|
  t.datetime 'date_of_birth'
  t.string   'country'
  t.string   'hometown'
  t.string   'live_at'
  t.string   'work'
  t.string   'interests'
  t.integer  'account_id'
  t.datetime 'created_at'
  t.datetime 'updated_at'
end
